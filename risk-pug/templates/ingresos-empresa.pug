extends ../fragments/page.pug
include ../mixins/panel.pug

block vars
  - title = 'Ingresos por Empresa'

block content
  include ../fragments/menus.pug
  - 
    breadcrumbLinks = [
      ['#', 'Inicio'],
      ['#', 'Ingresos']
    ];

  +panel('RESUMEN POR EMPRESA', breadcrumbLinks, panelLinks)
    .table-header
      .row.align-items-end.gap-2
        .col.col-auto
          section.hstack.mt-3
            .me-2
              span.text-nowrap Ingresos Acumulables
              br
              span Declaración Anual
            .rounded.overflow-hidden.hstack.position-relative.align-middle
              .bg-body-secondary.text-white.h-100.d-flex.justify-content-center.align-items-center.p-2
                i.material-symbols-outlined paid
              .text-bg-info.h-100.d-flex.justify-content-end.align-items-center.px-3.py-2(style='min-width: 120px;')
                span#ingresos-acumulables-anual 0.00
        .ms-auto.table-actions.hstack.align-items-end.gap-2.col-12.col-xl-auto
          button.ms-auto.btn.btn-success.btn-sm.text-nowrap#actualizar-informacion(type='button')
            i.material-symbols-outlined.align-bottom.me-1 autorenew
            span Actualizar Información
          .vr.mx-2
          form.hstack.gap-2.align-items-end#filter-table
            fieldset.ejercicio-field
              label.form-label(for='ejercicio') Ejercicio:
              .input-group.input-group-sm#ejercio-input-gr
                span.input-group-text.bg-white.border-end-0.material-symbols-outlined.cursor-pointer(data-td-target='#ejercio-input-gr' data-td-toggle='datetimepicker') calendar_month
                input.form-control.text-end.date#ejercicio(name='ejercicio' type='number' placeholder='_ _ _ _' data-td-target='#ejercio-input-gr' min='2000', required)
              script.

                document.addEventListener('DOMContentLoaded', function () {
                  const inputGrEjercicio = document.getElementById('ejercio-input-gr');
                  const inputEjercicio = document.querySelector('#ejercio-input-gr input');
                  const today = new Date();
                  inputEjercicio.setAttribute('max', today.getFullYear());
                  new tempusDominus.TempusDominus(inputGrEjercicio, {
                    display: {
                      components: {
                        decades: false,
                        year: true,
                        month: false,
                        date: false,
                        hours: false,
                        minutes: false,
                        seconds: false
                      },
                      //theme: 'light'
                    },
                    localization: {
                      format: 'yyyy',
                    },
                    restrictions: {
                      minDate: '2000',
                      maxDate: new Date()
                    }
                  });
                });

            button.btn.btn-primary.btn-sm.px-3.text-nowrap(type='submit')
              i.material-symbols-outlined.align-bottom filter_alt
              | Filtrar

            script.

              document.addEventListener('DOMContentLoaded', function () {
                const form = document.querySelector('#filter-table');
                form.addEventListener('submit', function (event) {
                  event.preventDefault();

                  const formData = new FormData(form);
                  formData.forEach((value, key) => {
                    console.log(`${key}: ${value}`);
                  });
                  const formObject = Object.fromEntries(formData.entries());
                  console.log(formObject);
                });
              });

          fieldset.time-process-component
            label.form-label Tiempo del Proceso:
            .input-group.input-group-sm
              span.input-group-text.bg-white.border-end-0.material-symbols-outlined update
              input.form-control.text-center#time-process(name='time-process' type='text' placeholder='-------------', readonly, style='box-shadow:none;')
